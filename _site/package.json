{
  "name": "dotnetsoutheast.github.io",
  "version": "1.0.0",
  "description": ".NET South East website",
  "main": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "npm run build && gh-pages -t -d www -b gh-pages",
    "clean": "rimraf www",
    "mydev": "npm run clean && npm run jekyll && npm run webpack:dev",
    "criticalinline": "inline-critical --html ./www/index.html --css ./www/assets/css/main.css > ./www/index.html",
    "dev": "npm run clean && concurrently 'npm run jekyll:dev' 'npm run webpack:dev'",
    "build": "npm run clean && npm run jekyll && npm run webpack && npm run criticalinline",
    "jekyll": "cross-env JEKYLL_ENV=production jekyll build --config config.yml",
    "jekyll:dev": "jekyll serve --config config.yml --trace --verbose",
    "stylesheets": "npm run stylesheets:compile && npm run stylesheets:postcss",
    "stylesheets:dev": "node-sass assets/scss/main.scss --output-style=compressed --output=www/assets/css/ --watch",
    "stylesheets:compile": "node-sass assets/scss/main.scss --output-style=compressed --output=www/assets/css/",
    "stylesheets:postcss": "postcss --no-map www/assets/css/main.css --output=www/assets/css/main.css",
    "webpack": "cross-env NODE_ENV=production webpack --progress --config webpack.config.js",
    "webpack:dev": "webpack --config webpack.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotnetsoutheast/dotnetsoutheast.github.io.git"
  },
  "author": "James Wragg <me@jameswragg.com> (http://jameswragg.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dotnetsoutheast/dotnetsoutheast.github.io/issues"
  },
  "homepage": "https://github.com/dotnetsoutheast/dotnetsoutheast.github.io#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-runtime": "^6.26.0",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "gh-pages": "^1.0.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "postcss-cli": "^4.1.1",
    "postcss-loader": "^2.0.6",
    "resolve-url-loader": "^2.1.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-critical": "^1.0.0",
    "webpack-shell-plugin": "^0.5.0",
    "webpack-svg-sprite-plugin": "^1.2.0"
  },
  "dependencies": {
    "underscore-template-loader": "^0.8.0"
  },
  "browserslist": ["> 1%", "last 2 versions", "not ie <= 8"]
}
